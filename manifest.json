{
  "name": "Histree",
  "version": "1.0",
  "description": "View your browsing history as a tree",
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
  "manifest_version": 2
}